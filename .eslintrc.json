{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@emotion"
    ],
    "rules": {
        "@emotion/jsx-import": "error"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}